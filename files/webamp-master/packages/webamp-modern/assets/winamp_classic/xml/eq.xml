<groupdef id="shadeeq">
    <layer id="blah" dblClickAction="SWITCH;normal" x="0" y="0" h="14" w="275" image="eq.shade.on" inactiveimage="eq.shade.off" move="1"/>
    <button id="winshade" inactivealpha="0" action="SWITCH" param="normal" x="254" y="3" image="eqshade.shade" downImage="eqshade.shadep"/>
    <button id="Close" inactivealpha="0" action="close" x="264" y="3" image="eqshade.close" downImage="eqshade.closep"/>
    <slider id="volume" action="volume" x="61" y="4" h="7" w="97" thumb="eqshade.vol.seekm"/>
    <slider id="balance" action="pan" x="164" y="4" h="7" w="43" thumb="eqshade.bal.seekm"/>
</groupdef>

<container id="EQ" name="Equalizer" default_x="0" default_y="116" default_visible="1">
    <layout id="normal" w="275" h="116" background="eq.bg">
        <group id="eq-content" x="0" y="0" w="0" h="0" relatw="1" relath="1" />
    </layout>

    <layout id="eq-shade" background="eq.shade.on" minimum_h="14" minimum_w="275">
		<group id="shadeeq" x="0" y="0" sysregion="1"/>
	</layout>
</container>